{
  "name": "css-modules-treeshaking",
  "version": "0.0.3",
  "private": false,
  "type": "module",
  "files": [
    "dist/"
  ],
  "types": "./dist/src/index.d.ts",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js"
    }
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
